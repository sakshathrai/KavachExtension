{
  "manifest_version": 3,
  "name": "Kavach Redirect Blocker",
  "version": "1.0",
  "description": "Kavach Redirect blocker testing.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  }
}
